#include<stdio.h>
int main(){
    int unit;
    float bill = 0;
    printf("enter unit:");
    scanf("%d", &unit);
    switch(unit/100) {
        case 0:
        bill = unit * 5;
        break;
        case 1:
        bill = unit * 5 + (unit-100) * 7;
        break;
        default:
        bill = unit * 5 + (unit-100) * 7 + (unit-200) * 10;
        break;
    }
        printf("Your electricity bill is: %.2f\n", bill);
        
    return 0;
}
