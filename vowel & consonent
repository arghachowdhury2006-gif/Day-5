#include<stdio.h>
int main() {
	char alphabet;
	printf("Enter alphabet:");
	scanf("%c", &alphabet);
	switch(alphabet){
	
	
	case'a':
		 printf("it is a vowel");
		break;
		case'e':
			 printf("it is a vowel");
		break;
		case'i':
			 printf("it is a vowel");
		break;
		case'o':
			 printf("it is a vowel");
		break;
		case'u':
			 printf("it is a vowel");
		break;
		default:
		printf("The alphabet is consonent");
	}
		
		return 0;
	
}
